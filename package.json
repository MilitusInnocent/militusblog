{
  "name": "starter-kit",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.13.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "cross-env DEBUG=express-sequelize nodemon ./bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "debug": "^3.2.7",
    "ejs": "^3.0.1",
    "engines": "^0.4.0",
    "express": "^4.16.2",
    "express-ejs-layouts": "^2.5.0",
    "morgan": "^1.7.0",
    "pg": "^6.4.2",
    "sequelize": "^3.35.1",
    "sequelize-cli": "^3.2.0",
    "serve": "^11.3.2",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "cross-env": "^3.2.4",
    "ejs": "^3.0.1",
    "expect.js": "^0.3.1",
    "mocha": "^3.5.3",
    "nodemon": "^2.0.2",
    "supertest": "^2.0.1"
  },
  "description": "This repository demonstrates the usage of Sequelize within an [Express](https://expressjs.com) application. The implemented logic is a simple task tracking tool.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://manifestandcompany@bitbucket.org/manifestcompany/starter-kit.git"
  },
